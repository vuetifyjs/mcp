{
  "name": "@vuetify/mcp",
  "version": "0.0.1",
  "description": "Machine Context Protocol server for Vuetify assistance",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "pnpm watch && node --watch dist/index.js",
    "cli": "node dist/cli/index.js",
    "bootstrap": "node --run build && node --run cli",
    "watch": "tsc --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "keywords": [
    "vuetify",
    "mcp",
    "claude",
    "anthropic"
  ],
  "author": "john@vuetifyjs.com",
  "license": "MIT",
  "dependencies": {
    "@clack/prompts": "^0.10.1",
    "@modelcontextprotocol/sdk": "^1.10.2",
    "@types/deepmerge": "^2.2.3",
    "deepmerge": "^4.3.1",
    "dotenv": "^16.5.0",
    "jsonc-parser": "^3.3.1",
    "kolorist": "^1.8.0",
    "octokit": "^4.1.3",
    "pathe": "^2.0.3",
    "std-env": "^3.9.0",
    "which": "^5.0.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/node": "^22.15.2",
    "@types/which": "^3.0.4",
    "eslint": "^9.25.1",
    "eslint-config-vuetify": "4.0.0-3",
    "typescript": "^5.8.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "unrs-resolver"
    ]
  },
  "packageManager": "pnpm@10.9.0"
}
